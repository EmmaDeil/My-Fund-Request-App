# Frontend Environment Configuration Examples
# Copy this file to .env, .env.development, or .env.production and configure values

# Environment (development|production)
REACT_APP_ENV=development

# API Configuration
# Development: http://localhost:5000/api
# Production: https://your-backend-domain.com/api
REACT_APP_API_URL=http://localhost:5000/api

# App Configuration
REACT_APP_NAME="Fund Request System"
REACT_APP_VERSION="1.0.0"

# Features
REACT_APP_PDF_ENABLED=true
REACT_APP_DEBUG=true

# Development Settings (only for development)
REACT_APP_LOG_LEVEL=debug
REACT_APP_SHOW_DEV_TOOLS=true